{
  "name": "schoolees/laravel-psgc",
  "description": "PSGC for Laravel: migrations, seeders, JSON data, models, services, resources, controllers, and read-only routes.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "psgc",
    "philippines",
    "geographic",
    "regions",
    "provinces",
    "cities",
    "barangays",
    "dataset"
  ],
  "homepage": "https://github.com/schoolees/laravel-psgc",
  "support": {
    "issues": "https://github.com/schoolees/laravel-psgc/issues",
    "source": "https://github.com/schoolees/laravel-psgc"
  },
  "authors": [
    {
      "name": "Schoolees",
      "email": "support@schoolees.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^12.0",
    "illuminate/database": "^12.0",
    "illuminate/routing": "^12.0",
    "illuminate/http": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "Schoolees\\Psgc\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Schoolees\\Psgc\\Providers\\PsgcServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
